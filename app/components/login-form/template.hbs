{{yield}}
<h2>Login as admin</h2>
<div class="col-md-4">
  <form>
    <div class="form-group">
      <label for="email">Email</label> {{input value=email id="email" class="form-control"}}
    </div>

    <div class="form-group">
      <label for="password">Password</label> {{input value=password id="password" class="form-control" type="password"}}
    </div>
   </form>
  <button name="button" class="btn" type="submit"{{action 'logIn'}}>Login</button>
</div>
